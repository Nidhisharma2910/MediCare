<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondActivity">

    <TextView
        android:id="@+id/doct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="144dp"
        android:text="@string/doctor"
        android:textColor="@color/black"
        android:textSize="45sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.008" />

    <EditText
        android:id="@+id/searchSome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="96dp"
        android:background="@drawable/email_margin"
        android:ems="10"
        android:hint="@string/search_doctor"
        android:inputType="text"
        android:padding="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doct"
        app:layout_constraintVertical_bias="0.013" />

    <ImageView
        android:id="@+id/dots"
        android:layout_width="23dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/searchSome"
        app:layout_constraintEnd_toEndOf="@+id/searchSome"
        app:layout_constraintHorizontal_bias="0.935"
        app:layout_constraintStart_toStartOf="@+id/searchSome"
        app:layout_constraintTop_toTopOf="@+id/searchSome"
        app:layout_constraintVertical_bias="0.566"
        app:srcCompat="@drawable/dots" />


    <TextView
        android:id="@+id/recommend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="264dp"
        android:background="@drawable/recom"
        android:padding="10dp"
        android:text="@string/recommended"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchSome"
        app:layout_constraintVertical_bias="0.021" />

    <TextView
        android:id="@+id/nearby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/email_margin"
        android:padding="10dp"
        android:text="@string/nearby"
        android:textColor="@color/black"
